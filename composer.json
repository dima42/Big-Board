{
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/santheo/php-slack"
    }
  ],
  "require": {
    "php": "~7.1.0",
    "silex/silex": "^2.0.4",
    "monolog/monolog": "^1.22",
    "twig/twig": "^2.0",
    "symfony/twig-bridge": "^3",
    "cocur/slugify": "^3.0",
    "propel/propel": "~2.0@dev",
    "klein/klein": "^2.1",
    "duzun/hquery": "^1.7",
    "nexylan/slack": "^3.1",
    "maximebf/debugbar": "^1.14",
    "aptoma/twig-markdown": "^3.0",
    "michelf/php-markdown": "^1.7",
    "guzzlehttp/guzzle": "~6.0",
    "frlnc/php-slack": "dev-no-format-support",
    "sentry/sentry": "^1.8",
    "twig/extensions": "^1.5",
    "google/apiclient": "^2.0"
  },
  "require-dev": {
    "deployer/deployer": "^6.0",
    "heroku/heroku-buildpack-php": "*"
  },
  "config-dev": {
    "bin-dir": "/usr/local/bin"
  },
  "autoload": {
    "classmap": [
      "generated-classes/"
    ]
  }
}
